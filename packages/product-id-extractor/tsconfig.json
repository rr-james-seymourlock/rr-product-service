{
  "extends": "@rr/tsconfig/base.json",
  "compilerOptions": {
    "tsBuildInfoFile": "node_modules/.cache/tsbuildinfo.json",
    "paths": {
      "@rr/url-parser": ["../url-parser/src"],
      "@rr/url-parser/*": ["../url-parser/src/*"],
      "@rr/store-registry": ["../store-registry/src"],
      "@rr/store-registry/*": ["../store-registry/src/*"]
    }
  },
  "include": ["src"],
  "exclude": ["node_modules", "dist"]
}
